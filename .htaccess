AuthType Basic
AuthName "Password Protected Area"
AuthUserFile H:/xampp/htdocs/condor_web/.htpasswd
Require valid-user

RedirectMatch permanent ^\/posts\/(.+\.(css|js|png)+)$  /$1

<IfModule mod_rewrite.c>
Options +FollowSymlinks
RewriteEngine On
RewriteBase /condor_web

RewriteRule ^(posts|events|cvl|transport|maps|canteen)$ $1/
RewriteRule ^(.+)\/$ index.php?module=$1 [L]
RewriteRule ^(.+)\/(.+)\.(.+)$ $2.$3 [L]
RewriteRule ^posts\/(\d+)$ index.php?module=posts&id=$1 [L]
RewriteRule ^maps\/([a-z]+)$ index.php?module=maps&id=$1 [L]

</IfModule>